<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Images</os:ShortName>
<os:Description>Google Images</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://encrypted.google.com/imghp</SearchForm>
<os:Url type="text/html" method="GET" template="https://www.google.com/search">
  <os:Param name="site" value="imghp"/>
  <os:Param name="tbm" value="isch"/>
  <os:Param name="source" value="hp"/>
  <os:Param name="biw" value="1280"/>
  <os:Param name="bih" value="879"/>
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="btnG" value="Recherche+par+image"/>
  <os:Param name="oq" value=""/>
  <os:Param name="gs_l" value=""/>
</os:Url>
</SearchPlugin>