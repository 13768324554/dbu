<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Ixquick</os:ShortName>
<os:Description>Ixquick</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://www.ixquick.com/</SearchForm>
<os:Url type="text/html" method="POST" template="https://www.ixquick.com/do/search">
  <os:Param name="cat" value="web"/>
  <os:Param name="cmd" value="process_search"/>
  <os:Param name="language" value="english"/>
  <os:Param name="engine0" value="v1all"/>
  <os:Param name="query" value="{searchTerms}"/>
  <os:Param name="abp" value="1"/>
  <os:Param name="src" value=""/>
</os:Url>
</SearchPlugin>