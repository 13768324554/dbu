label live-@FLAVOUR@-english
        menu label ^Live (@FLAVOUR@) (English)
        menu default
        linux @LINUX@
        initrd @INITRD@
        append @APPEND_LIVE@ locales=en_US.UTF-8

label live-@FLAVOUR@-french
        menu label ^Live (@FLAVOUR@) (French)
        menu default
        linux @LINUX@
        initrd @INITRD@
        append @APPEND_LIVE@ locales=fr_FR.UTF-8
